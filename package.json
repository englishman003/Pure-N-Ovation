{
  "name": "purenovation",
  "version": "1.0.0",
  "description": "My Personal Website",
  "main": "JS/app.js",
  "scripts": {
    "WatchSass": "node-sass  SCSS/Main/main.scss CSS/purenovation.css -w",
    "DevServer": "live-server",
    "LocalServer": "nodemon JS/app.js",
    "Start": "npm-run-all --parallel DevServer WatchSass",
    "CompileSass": "node-sass SCSS/Main/main.scss CSS/purenovation.comp.css",
    "ConcatCSS": "concat -o CSS/purenovation.concat.css SCSS/Vendors/Animate.css SCSS/Vendors/Normalize.css CSS/purenovation.comp.css",
    "PrefixCSS": "postcss --use=autoprefixer -b \"last 20 versions\" CSS/purenovation.concat.css -o CSS/purenovation.prefix.css",
    "CompressCSS": "node-sass CSS/purenovation.prefix.css CSS/purenovation.css --output-style compressed",
    "BuildCSS": "npm-run-all CompileSass ConcatCSS PrefixCSS CompressCSS"
  },
  "author": "Nathan Cluff",
  "license": "ISC",
  "repository": "https://github.com/englishman003/Pure-N-Ovation",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "express": "^4.17.3",
    "jquery": "^3.4.1",
    "mongoose": "^5.9.4",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.5",
    "nodemailer-mailgun-transport": "^2.0.0",
    "nodemon": "^2.0.2",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postman": "^0.2.0",
    "prettier": "^1.19.1",
    "vee-validate": "^3.2.5",
    "vue": "^2.6.11",
    "xoauth2": "^1.2.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express-validator": "^6.4.0",
    "html-to-text": "^5.1.1",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "require.js": "^1.0.0",
    "validator": "^12.2.0"
  }
}
