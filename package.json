{
  "name": "purenovation",
  "version": "1.0.0",
  "description": "My Personal Website",
  "main": "jQuery.v3.3.1.js",
  "scripts": {
    "WatchSass": "node-sass SCSS/Main/main.scss CSS/purenovation.css -w",
    "DevServer": "live-server",
    "Start": "npm-run-all --parallel DevServer WatchSass",
    "CompileSass": "node-sass SCSS/Main/main.scss CSS/purenovation.comp.css",
    "ConcatCSS": "concat -o CSS/purenovation.concat.css SCSS/Vendors/Animate.css SCSS/Vendors/Normalize.css CSS/purenovation.comp.css",
    "PrefixCSS": "postcss --use=autoprefixer -b \"last 20 versions\" CSS/purenovation.concat.css -o CSS/purenovation.prefix.css",
    "CompressCSS": "node-sass CSS/purenovation.prefix.css CSS/purenovation.css --output-style compressed",
    "BuildCSS": "npm-run-all CompileSass ConcatCSS PrefixCSS CompressCSS"
  },
  "author": "Nathan Cluff",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "express": "^4.17.1",
    "node-sass": "^4.13.1",
    "node.js": "0.0.0",
    "nodemailer": "^6.4.2",
    "nodemailer-mailgun-transport": "^2.0.0",
    "php": "0.0.1",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "vee-validate": "^3.2.5",
    "vue": "^2.6.11",
    "xoauth2": "^1.2.0"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "npm-run-all": "^4.1.5"
  }
}
